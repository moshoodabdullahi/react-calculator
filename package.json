{
  "name": "react-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview --strictPort --port 8080",
    "format": "npx prettier --write \"**/*.{html,json,md,yaml,yml,cjs,jsx,js,css}\" --cache --cache-location node_modules/.cache/.prettiercache --ignore-path .gitignore",
    "format:check": "npx prettier --check \"**/*.{html,json,md,yaml,yml}\" --cache --cache-location node_modules/.cache/.prettiercache --ignore-path .gitignore",
    "lint": "yarn stylelint & yarn eslint",
    "lint:check": "yarn stylelint:check & yarn eslint:check",
    "stylelint": "npx stylelint \"**/*.css\" --fix --cache --cache-location node_modules/.cache/.stylelintcache",
    "stylelint:check": "npx stylelint \"**/*.css\" --cache --cache-location node_modules/.cache/.stylelintcache",
    "eslint": "npx eslint . --report-unused-disable-directives --fix --cache --cache-location node_modules/.cache/.eslintcache",
    "eslint:check": "npx eslint . --report-unused-disable-directives --cache --cache-location node_modules/.cache/.eslintcache",
    "axe": "node axe-check.js",
    "lint:html": "htmlhint index.html",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "vite": "^5.0.8",
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.1",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.8.2",
    "axe-puppeteer": "^1.1.1",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.33",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "puppeteer": "^21.4.1",
    "stylelint": "^16.1.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0",
    "tailwindcss": "^3.4.1"
  }
}
